{
  "name": "webpack-tutor",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node server.bundle.js",
    "start-dev": "concurrently --kill-others \"npm run api-dev\" \"npm run web-dev\" \"npm run watch-webpack\" \"npm run watch-webpack-server\"",
    "api-dev": "nodemon ./api --watch ./api",
    "web-dev": "concurrently --kill-others \"nodemon server.bundle.js --watch server.bundle.js\"",
    "watch-webpack": "webpack -w",
    "watch-webpack-server": "webpack -w --config webpack.server.config.js",
    "build": "npm run build-webpack && npm run build-webpack-server",
    "build-webpack": "NODE_ENV=production webpack",
    "build-webpack-server": "NODE_ENV=production webpack --config webpack.server.config.js",
    "test": "NODE_ENV=test mocha"
  },
  "author": "David Hunt <davidpaulhunt@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "concurrently": "^2.1.0",
    "cookie-parser": "^1.4.3",
    "express": "^4.13.4",
    "react-router": "^2.4.1"
  },
  "devDependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.12.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^1.4.2",
    "eslint-plugin-react": "^5.1.1",
    "nodemon": "^1.9.2",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1"
  }
}
